{
  "name": "dj-cloudio",
  "version": "1.0.0",
  "description": "AI-Powered Prophetic DJ with Web3 Integration",
  "main": "neuralmix_enhanced_fixed.html",
  "bin": {
    "claude": "./claude-cli.js",
    "claude-cli": "./claude-cli.js"
  },
  "scripts": {
    "start": "npx serve . -p 3000",
    "start:cosmogram": "npx serve . -p 3001 --open cosmogram.html",
    "test": "cd contracts && npx hardhat test",
    "coverage": "cd contracts && npx hardhat coverage",
    "lint": "eslint . --ext .js,.html --max-warnings 0",
    "lint:fix": "eslint . --ext .js,.html --fix",
    "format": "prettier --write \"**/*.{js,html,css,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,html,css,json,md}\"",
    "compile": "cd contracts && npx hardhat compile",
    "deploy:testnet": "npm run validate:env:contracts && cd contracts && npx hardhat run scripts/deploy.js --network baseSepolia && npm run postdeploy:testnet && npm run report:testnet && npm run mint-nft:testnet",
    "deploy:mainnet": "npm run validate:env:contracts && cd contracts && npx hardhat run scripts/deploy.js --network base && npm run postdeploy:mainnet && npm run report:mainnet && npm run mint-nft:mainnet",
    "postdeploy:testnet": "node contracts/scripts/postDeploy.js baseSepolia",
    "postdeploy:mainnet": "node contracts/scripts/postDeploy.js base",
    "postdeploy": "node contracts/scripts/postDeploy.js",
    "report:testnet": "node contracts/scripts/generateReport.js baseSepolia",
    "report:mainnet": "node contracts/scripts/generateReport.js base",
    "report": "node contracts/scripts/generateReport.js",
    "mint-nft:testnet": "node contracts/scripts/mintDeploymentNFT.js baseSepolia",
    "mint-nft:mainnet": "node contracts/scripts/mintDeploymentNFT.js base",
    "mint-nft": "node contracts/scripts/mintDeploymentNFT.js",
    "validate:env": "node validate-env.js frontend",
    "validate:env:contracts": "node validate-env.js contracts --contracts",
    "validate:security": "bash test-gitignore.sh && node validate-env.js --security-only",
    "prestart": "node validate-env.js frontend --skip-deployment",
    "claude": "node claude-cli.js",
    "claude:analyze": "node claude-cli.js analyze",
    "claude:test": "node claude-cli.js test",
    "claude:fix": "node claude-cli.js fix",
    "claude:review": "node claude-cli.js review && gh workflow run code-quality.yml -f task=code-quality-only",
    "claude:security": "node claude-cli.js security && gh workflow run code-quality.yml -f task=security-only",
    "claude:optimize": "node claude-cli.js optimize",
    "claude:docs": "node claude-cli.js docs",
    "claude:status": "node claude-cli.js status",
    "oracle": "node oracle-cli.js",
    "oracle:divine": "node oracle-cli.js divine",
    "oracle:consult": "node oracle-cli.js consult",
    "oracle:prophecy": "node oracle-cli.js prophecy",
    "oracle:next": "node oracle-cli.js next",
    "glyph": "node glyph-cli.js",
    "glyph:generate": "node glyph-cli.js generate",
    "glyph:list": "node glyph-cli.js list",
    "glyph:batch": "node glyph-cli.js batch",
    "glyph:preview": "node glyph-cli.js preview"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fullmeo/-Neural_claude_code.git"
  },
  "keywords": [
    "dj",
    "ai",
    "web3",
    "blockchain",
    "audio",
    "mixing",
    "nft",
    "dao"
  ],
  "author": "fullmeo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fullmeo/-Neural_claude_code/issues"
  },
  "homepage": "https://github.com/fullmeo/-Neural_claude_code#readme",
  "devDependencies": {
    "eslint": "^8.56.0",
    "prettier": "^3.1.1",
    "serve": "^14.2.1"
  },
  "dependencies": {
    "web3": "^4.3.0"
  }
}
